#8
egrep -i "^[^aeiouy]{2,}$" < web2

#9
sed -r 's/(.*):.*:(.*):.*:.*:.*:(.*)$/user: \1\tuid: \2\tshell: \3/g' < /etc/passwd

#10
awk '{printf "%d %s\n" , NR, $0}' < alice
