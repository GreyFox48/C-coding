/**
 * @file fizzbuzz.c
 * @brief Fizzbuzz from 1 to 100
 * @details Fizz on multiples of 3, buzz for of 5, fizzbuzz for both 3 and 5
 *
 * @author Matthew Olsen
 * @date February 7, 2016
 * @todo Nothing
 * @bug none
 */

#include <stdio.h>

int main(void)
{
        int i;
        
        for (i = 1; i <= 100; i++) {
                if (i % 3 == 0 && i % 5 == 0){
                        printf("FizzBuzz\n");
                } else if (i % 5 == 0) {
                        printf("Buzz\n");
                } else if (i % 3 == 0) {
                        printf("Fizz\n");
                } else {
                        printf("%d\n", i);
                }                
        }
        
        return 0;
}
